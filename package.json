{
  "name": "Grape",
  "version": "3.0.0-alpha.8",
  "description": "The world’s most advanced team chat meets enterprise search.",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=development electron .",
    "build": "electron-builder",
    "deploy": "electron-builder --publish onTagOrDraft"
  },
  "repository": "https://github.com/ubergrape/grape-electron",
  "keywords": [
    "Grape",
    "chat",
    "enterprise chat"
  ],
  "author": "UberGrape GmbH <support@chatgrape.com>",
  "copyright": "© 2019, UberGrape GmbH",
  "build": {
    "appId": "com.ChatGrape",
    "buildVersion": "56",
    "mac": {
      "category": "public.app-category.business",
      "target": [
        "mas",
        "dmg",
        "zip"
      ]
    },
    "linux": {
      "target": [
        "tar.gz",
        "deb",
        "rpm",
        "pacman"
      ],
      "executableName": "Grape",
      "artifactName": "${productName}-${version}.${ext}",
      "category": "Office"
    },
    "directories": {
      "output": "build",
      "buildResources": "assets"
    }
  },
  "devDependencies": {
    "electron": "^6.0.2",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "electron-log": "^3.0.7",
    "electron-updater": "^4.1.2"
  }
}
